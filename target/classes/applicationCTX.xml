<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
   
   <!-- Calculator 클래스로 calculator  객체 생성 -->
   <bean id="cal" class="com.kmii.test.Calculator"> </bean>
   
    <!-- MyCalculator 클래스로 myCalculator  객체 생성 -->
   <bean id="mycalculator" class="com.kmii.test.Mycalculator">
   	<property name="a" value="10"></property>
   	<property name="b" value="5"></property>
   	<property name="calculator">
   		<ref bean="cal" />
   	</property>
   		
    </bean>
   
</beans>